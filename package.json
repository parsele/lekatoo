{
  "name": "green-planet-tree-tracker",
  "version": "1.0.0",
  "description": "Tree tracking application for Amboseli",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
